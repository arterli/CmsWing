    "dataloader": "^2.2.1",
    "egg": "^3.15.0",
    "egg-cors": "^2.2.3",
    "egg-downloader": "^1.0.5",
    "egg-graphql": "^2.8.0",
    "egg-remote-config": "^1.0.1",
    "egg-scripts": "^2.17.0",
    "egg-sequelize": "^6.0.0",
    "egg-swagger-docs": "^2.3.5",
    "egg-view-nunjucks": "^2.3.0",
    "esdk-obs-nodejs": "^3.21.6",
    "fs-extra": "^10.1.0",
    "graphql": "^16.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "qiniu": "^7.7.0",
    "utility": "^1.17.0"
  },
  "devDependencies": {
    "autod": "^3.1.2",
    "autod-egg": "^1.1.0",
    "egg-bin": "^6.0.0",
    "egg-ci": "^2.2.0",
    "egg-mock": "^5.10.4",
    "eslint": "^8.34.0",
    "eslint-config-egg": "^12.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-CmsWing",
